{
    "name": "dvlpp/sharp",
    "description": "A kind of CMS for developers who hate CMS",
    "authors": [{
        "name": "Philippe Lonchampt",
        "email": "philippelonchampt@gmail.com"
    }],
    "require": {
        "php": ">=5.5.9",
        "laravel/framework": "5.2.*|5.3.*|5.4.*|5.5.*",
        "laravelcollective/html": "5.2.*|5.3.*|5.4.*|5.5.*",
        "intervention/image": "2.*",
        "erusev/parsedown": "~1.1",
        "mustache/mustache": "~2.6.0"
    },
    "require-dev": {
        "laravel/laravel": "5.5.*",
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~6.0",
        "phpspec/phpspec": "~2.1",
        "symfony/css-selector": "2.8.*|3.0.*",
        "symfony/dom-crawler": "2.8.*|3.0.*",
        "doctrine/dbal": "~2.5"
    },
    "autoload": {
        "files": [
            "src/helpers/sharp_helper.php",
            "src/helpers/sharp_auth_helper.php",
            "src/helpers/sharp_config_helper.php"
        ],
        "psr-0": {
            "Dvlpp\\Sharp": "src/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    }
}
